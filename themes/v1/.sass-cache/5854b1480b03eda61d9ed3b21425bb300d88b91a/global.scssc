html,body,div,span,applet,object,iframe,a,abbr,acronym,address,big,cite,code,del,dfn,em,font,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,dl,dt,dd,ol,ul,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,textarea,label,legend,p,blockquote,table,caption,tbody,tfoot,thead,tr,th,td{margin:0;padding:0;border:0;outline:0;font-weight:inherit;font-style:inherit;font-size:100%;font-family:inherit;vertical-align:baseline;}body{line-height:1;color:black;background:white;}:focus{outline:0;}table{border-collapse:collapse;border-spacing:0;}caption,th,td{text-align:left;font-weight:normal;}fieldset,img{border:0;}address,caption,cite,code,dfn,em,strong,th,var{font-style:normal;font-weight:normal;}ol,ul{list-style:none;}h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:normal;}blockquote:before,blockquote:after,q:before,q:after{content:"";}blockquote,q{quotes:"" "";}abbr,acronym{border:0;}

a {text-decoration: none !important;}

* {
  
  -webkit-box-sizing: border-box;
     -moz-box-sizing: border-box;
          box-sizing: border-box;

}

/* END RESET */

@import "bits";
@import "type";
@import "compass/css3";

// Reusable Classes 

.screen-reader-text {
  position: absolute;
  top: -9999px;
  left: -9999px;
}

// Semi-Global Styles 
a {
    color: $linkColor;
    @include transition(all .2s ease-in-out);
    &:hover {
      color: darken($linkColor, 10%);
    }
  }

html {
  margin-top: 0 !important;
}

body {
  font-size: 110%;
  background: $cream;
  color: #c9d3da;
  @include sans-type;
}

// Header Styles

header {
  background: $mainColor;
  padding: 30px 0;
  @include sans-type;
  img {
    float: left;
    margin-right: 15px;
  }
  h1 {
    margin-top: 15px;
    font-size: 1.5em;
  }
  a {
    color: white;
  }
}

.logo {
  display: inline-block;
  img {
    width: 60px; height: 60px;
  }
  h1 {
    display: inline-block;
  }
  &:hover {
    @include opacity(.7);
    color: white;
  }
}

nav {
  float: right;
  margin-top: 24px;
  font-size: .9em;
  a {
    color: lighten($mainColor, 25%);
    &:hover {
      color: lighten($mainColor, 50%);
    }
  }
}

.contain {
  max-width: 940px;
  margin: 0 auto;
  overflow: hidden;
}

// Main Content Styles

.main-content {
  max-width: 940px;
  margin: 50px auto;
  color: darken($gray, 5%);
  h2 {
    text-transform: uppercase;
    color: $mainColor;
    font-weight: bold;
    margin-bottom: 40px;

  }
}

.statement {
  background: $linkColor;
  padding: 40px 0;
  color: white;
  h1 {
    line-height: 1.4;
    text-align: center;
    font-size: 2.2em;
  }
  a {
    color: white;
    &:hover {
      color: rgba(white, .7);
    }
  }
}

.entry {
  background: white;
  @include border-radius(3px);
  box-shadow: 0 3px 0 rgba(black, .10);
  overflow: hidden;
  margin-bottom: 50px;

  img {
    float: left;
    margin-right: 40px;
    width: 100%; height: auto;
    max-width: 300px;
  }
  .text {
    padding: 40px 40px 40px 0;
    overflow: hidden;
    h3 {
      font-size: 1.8em;
      margin-bottom: 15px;
    }
  }
  .button {
    margin-top: 20px;
    float: right;
    font-size: .95em;
    background: $green;
    color: white;
    padding: 10px 15px;
    @include border-radius(3px);
    &:hover {
      background: $gray;
      color: white;
    }
  }
}

// Single Page Styles

.price {
  border-bottom: 1px solid #ddd;
  padding-bottom: 20px;
  margin-bottom: 40px;
  p {
    font-weight: bold;
    color: #535353;
    font-size: 1.5em !important;
    display: inline-block;
    margin: 0 !important;
  }
  .button {
    margin: 0;
  }
}

.col-1 {
  width: 34.8837209%;
  margin-right: 40px;
  float: left;
  h3 {
    font-size: .95em;
    font-weight: bold;
    color: #535353;
    margin-bottom: 5px;
  }
  p {
    font-size: .85em;
    margin-bottom: 30px;
  }
  img {
    float: none;
    margin: 0 0 30px 0;
  }
}

p {
  &.lead {
  color: darken($gray, 25%);
  font-size: 1.1em !important;
  }
}

hr {
  border: solid lighten($mainColor, 65%);
  border-width: 5px 0 0;
  clear: both;
  margin: 35px auto;
  height: 0;
  width: 50%;
  display: block;
  padding: 0;
}

.single {
  .entry {
    padding: 40px;
  }
  .text {
    padding: 0;
    p {
      font-size: .95em;
      margin-bottom: 15px;
    }
    h2 {
      color: $gray;
      font-size: 1.8em;
      margin-bottom: 15px;
      text-transform: none;
      font-weight: normal;
    }
  }
}

ul {
  margin: 0 0 15px 20px;
  list-style-type: disc;
  li {
    font-size: .95em;
    padding: 8px 0;
  }
}

// Help Page Styles 

.page {
  @extend .single;
  .entry {
  }
}

.content-col {
  width: 60.4651163%;
  margin-right: 04.6511628%;
  float: left;
  overflow: hidden;
  h2 {
    color: $gray;
    font-size: 1.8em;
    margin-bottom: 15px;
    text-transform: none;
    font-weight: normal;
    }
  p {
    font-size: .95em;
    margin-bottom: 15px;
  }
}

.sidebar {
  float: left;
  width: 34.8837209%;
  h3 {
    font-size: .95em;
    font-weight: bold;
    color: #535353;
    margin-bottom: 30px;
  }
  h4 {
    font-size: .87em;
    line-height: 1.4;
    font-weight: bold;
    color: #535353;
    margin-bottom: 5px;
  }
  p {
    font-size: .85em;
    margin-bottom: 30px;
  }
}

// Form Styles

.gform_fields {
  list-style-type: none;
  margin: 0;
}

.gfield {
  padding: 0;
  margin-bottom: 20px;
}

.gfield_label {
  margin-bottom: 10px;
  display: block;
  color: $mainColor;
  font-weight: bold;
  font-size: .87em;
}

.gfield_required {
  color: $linkColor;
}

.ginput_left {
  position: relative;
  width: 50%;   
  padding-right: 10px;
  display: inline-block;
  input {
    background: #f5f5f5;
    padding: 10px 0 10px 50px;
    border: 1px solid #f5f5f5;
    @include border-radius(3px);
    @include transition(all .3s ease-in-out);
    width: 100%;
    &:focus {
      background: white;
      border-color: lighten(#3d9ae8, 15%);
    }
  }
  label {
    color: #ccc;
    text-transform: uppercase;
    font-size: .65em;
    font-weight: bold;
    position: absolute;
    top: 5px;
    left: 5px;
  }
}

.ginput_right {
  position: relative;
  width: 50%;
  display: inline-block;
  input {
    background: #f5f5f5;
    padding: 10px 0 10px 50px;
    border: 1px solid #f5f5f5;
    @include border-radius(3px);
    @include transition(all .3s ease-in-out);
    width: 100%;
    &:focus {
      background: white;
      border-color: lighten(#3d9ae8, 15%);
    }
  }
  label {
    color: #bbb;
    text-transform: uppercase;
    font-size: .65em;
    font-weight: bold;
    position: absolute;
    top: 5px;
    left: 5px;
  }
}

.g_email {
  input {
    background: #f5f5f5;
    width: 100%;
    padding: 10px;
    border: 1px solid #f5f5f5;
    @include border-radius(3px);
    @include transition(all .3s ease-in-out);
    &:focus {
      background: white;
      border-color: lighten(#3d9ae8, 15%);
    }
  }
}

.g_message {
  textarea {
    background: #f5f5f5;
    width: 100%;
    height: 200px;
    padding: 10px;
    border: 1px solid #f5f5f5;
    @include border-radius(3px);
    @include transition(all .3s ease-in-out);
    resize: none;
    &:focus {
      background: white;
      border-color: lighten(#3d9ae8, 15%);
    }
  }
}

.gform_button {
  @include transition(background .2s ease-in-out);
}



// Footer Styles 

.newsletter {
  background: darken($cream, 5%);
  padding: 40px 0;
  border-top: 1px solid darken($cream, 10%);
  h2 {
    color: $mainColor;
    font-weight: bold;
    margin-bottom: 40px;
    text-transform: uppercase;
  }
}

.email-field {
  float: left;
  padding: 20px;
  width: 70%;
  @include border-radius(4px 0 0 4px);
  font-size: 1.2em;
  @include transition(box-shadow .2s ease-in-out);
  &:focus {
    box-shadow: inset 0 0 0 3px $linkColor;
  }
}

.submit {
  font-size: 1.2em;
  background: $linkColor;
  color: white;
  padding: 20px;
  width: 30%;
  @include border-radius(0 4px 4px 0);
  @include transition(background .2s ease-in-out);
  &:hover {
    background: lighten($linkColor, 5%);
  }
}

.about-me {
  background: darken($cream, 10%);
  border-top: 1px solid darken($cream, 15%);
  padding: 40px 0;
  img {
    @include border-radius(200px);
    border: 10px solid white;
    float: left;
    width: 23.4042553%;
  }
}

.about-me-text {
  margin-top: 30px;
  float: left;
  width: 74.4680851%;
  padding-left: 20px;
  h2 {
    color: $mainColor;
    font-weight: bold;
    margin-bottom: 30px;
    text-transform: uppercase;
  }
  p {
    color: darken($cream, 65%);
    font-size: .92em;
    &:nth-child(2) {
      font-size: 1.2em;
      margin-bottom: 5px;
      color: darken($cream, 75%);
    }
    &:nth-child(4) {
      font-size: .8em;
      margin-top: 10px;
    }
  }
  a {
    border-bottom: 1px solid $linkColor;
    &:hover {
      border-bottom-color: darken($linkColor, 10%);
    }
  }
}

footer {
  background: $mainColor;
  padding: 30px 0;
  color: white;
  font-size: .85em;
  a {
    color: white;
    &:hover {
      color: rgba(white, .7);
    }
  }
